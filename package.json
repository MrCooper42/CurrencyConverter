{
  "name": "currency",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start:dev": "set DEBUG=\"prisma*\" && cross-env NODE_ENV=development tsx watch src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "prepare": "husky",
    "lint:commit": "commitlint --edit",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "generateOpenapi": "npx openapi-typescript ./coinbase/swagger.json -o coinbase/openapi-schema.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "docker:build": "docker build -t currency .",
    "docker:start": "docker run -p 8080:8080 currency"
  },
  "dependencies": {
    "@prisma/client": "^6.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "i": "^0.3.7",
    "js-big-decimal": "^2.2.0",
    "lru-cache": "^11.0.2",
    "npm": "^11.1.0",
    "uuidv4": "^6.2.13",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/cors": "^2.8.17",
    "@types/express": "~4.17.13",
    "@types/jest": "^29.5.14",
    "@types/node": "~18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-extra": "^0.1.0",
    "eslint-plugin-extra-rules": "^0.0.0-development",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.24.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "openapi-fetch": "^0.13.4",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.2.5",
    "prisma": "^6.4.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.3.2"
  }
}
