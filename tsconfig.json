{
  "compilerOptions": {
	"outDir": "./dist",
	"baseUrl": ".",
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"strict": true,
	"sourceMap": true,
	"resolveJsonModule": true,
	"skipLibCheck": true,
	"paths": {
	  "@db/*": [
		"prisma/*"
	  ],
	  "@controllers/*": [
		"src/controllers/*"
	  ],
	  "@middlewares/*": [
		"src/middlewares/*"
	  ],
	  "@validators/*": [
		"src/validators/*"
	  ],
	  "@routes/*": [
		"src/routes/*"
	  ],
	  "@models/*": [
		"src/models/*"
	  ],
	  "@services/*": [
		"src/services/*"
	  ],
	  "@logger/*": [
		"src/logging/*"
	  ],
	  "@utils/*": [
		"src/utils/*"
	  ],
	  "@projTypes/*": [
		"src/types/*"
	  ]
	}
  },
  "exclude": [
	"**/*.test.ts"
  ]
}