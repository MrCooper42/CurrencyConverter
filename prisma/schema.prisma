generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  authToken String     @id
  lastLogin DateTime   @default(now())
  Requests  Request[]
  Responses Response[]
}

model Request {
  id       String     @id
  user     User       @relation(fields: [userId], references: [authToken])
  userId   String
  to       Currencies
  from     Currencies
  amount   Float
  date     DateTime   @default(now())
  Response Response[]
}

model Response {
  requestId String  @id
  request   Request @relation(fields: [requestId], references: [id])
  user      User    @relation(fields: [userId], references: [authToken])
  userId    String
  amount    String
  from      String
  to        String
}

